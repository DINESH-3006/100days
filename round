import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class round
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc=new Scanner(System.in);
        int ip=sc.nextInt();
        while(ip>0){
            int n=sc.nextInt();
            int x=sc.nextInt();
            if(x>=n && x%n==0)
            System.out.println("yes");
            else
            System.out.println("no");
            ip--;
        }
	}
}
