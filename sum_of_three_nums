import java.util.*;
class sum_of_three_nums {
    public static void main(String[] args) {
        
        Scanner sc=new Scanner (System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
System.out.println((a+b)+"\n");
System.out.println((a+c)+"\n");
System.out.println((b+c)+"\n");


        
    }
}
